# platform :ios, '9.0'

target 'basic' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  pod 'Alamofire', '~> 4.9.1'

  # Pods for basic

  target 'basicTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Hippolyte'
    pod 'Swifter', '~> 1.4.7'
  end

  target 'basicUITests' do
    # Pods for testing
    use_frameworks!
    pod 'Hippolyte'
    pod 'Alamofire', '~> 4.9.1'
    pod 'Swifter', '~> 1.4.7'
  end

end
